{
	"name": "turahe/laravel-likeable",
	"description": "Trait for Laravel Eloquent models to allow easy implementation of a 'like' or 'favorite' or 'remember' feature.",
	"license": "MIT",
	"type": "library",
	"keywords": ["trait", "laravel", "eloquent", "likeable", "likable", "like", "remember", "follow", "favorite", "favourite"],
	"authors": [
		{
			"name": "Nur Wachid",
			"email": "wachid@outlook.com",
			"homepage": "https://turahe.id",
            "role": "Developer"
		}
	],
	"homepage": "https://github.com/turahe/laravel-likeable",
	"require": {
		"php": "^7.3|^8.0",
		"illuminate/database": "5.* | ^6.0 | ^7.0 | ^8.0",
		"illuminate/support": "5.* | ^6.0 | ^7.0 | ^8.0"
	},
	"require-dev": {
        "friendsofphp/php-cs-fixer": "^3.1",
		"orchestra/testbench": "^5.0|^6.0",
		"phpunit/phpunit": "^8.5|^9.3",
		"mockery/mockery": "^1.3"
	},
	"autoload": {
		"psr-4": {
			"Turahe\\Likeable\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Turahe\\Tests\\Likeable\\": "tests/"
		}
	},
	"scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
	"extra": {
		"laravel": {
			"providers": [
				"Turahe\\Likeable\\LikeableServiceProvider"
			]
		}
	},
	"minimum-stability": "stable",
	"prefer-stable" : true
}
